{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsonRecursiveSort": true,
  "jsxSingleQuote": false,
  "plugins": ["prettier-plugin-sort-json"],
  "printWidth": 100,
  "proseWrap": "preserve",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
